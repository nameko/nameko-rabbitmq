[
  { rabbit,
    [
      { tcp_listeners, [ 5672 ] },
      { ssl_listeners, [ 5671 ] },
      { ssl_options, [ { cacertfile, "/srv/ssl/cacert.pem" },
                       { certfile, "/srv/ssl/servercert.pem" },
                       { keyfile, "/srv/ssl/serverkey.pem" },
                       { verify, verify_peer },
                       { fail_if_no_peer_cert, false } ] },
      { default_user, <<"guest">> },
      { default_pass, <<"guest">> },
      { loopback_users, [ ] }
    ]
  },
  { rabbitmq_management,
    [
      { listener, [ { port, 15672 },
                    { ssl, false } ] }
    ]
  }
].
